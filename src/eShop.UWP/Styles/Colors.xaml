<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Colors  -->
    <Color x:Key="PrimaryMedium">#00A59A</Color>
    <Color x:Key="PrimaryLight">#00CFC1</Color>
    <Color x:Key="SecondaryDark">#020202</Color>
    <Color x:Key="SecondaryMedium">#575757</Color>
    <Color x:Key="SecondaryLight">#CBCFCE</Color>
    <Color x:Key="TertiaryDark">#000000</Color>
    <Color x:Key="TertiaryLight">#FFFFFF</Color>
    <Color x:Key="InkColor">#A72B56</Color>
    <Color x:Key="ItemBackground">#52FFFFFF</Color>
    <Color x:Key="TimeRangeBackground">#0bc3b7</Color>

    <!--  Chart colors  -->
    <Color x:Key="PrimaryMediumChart">#00A59A</Color>
    <Color x:Key="PrimaryLightChart">#14EDDE</Color>
    <Color x:Key="SecondaryMediumChart">#E1428D</Color>
    <Color x:Key="SecondaryLightChart">#FF86C5</Color>
    <Color x:Key="TertiaryMediumChart">#8138C0</Color>

    <Color x:Key="TitleBarForegroundDark">#000000</Color>
    <Color x:Key="TitleBarForegroundInactive">LightGray</Color>
    <Color x:Key="TitleBarForegroundLight">#FFFFFF</Color>
    <Color x:Key="TitleBarBackground">Transparent</Color>
</ResourceDictionary>
