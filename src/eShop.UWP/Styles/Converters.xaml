<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters1="using:eShop.UWP.Converters"
    xmlns:converters="using:Microsoft.Toolkit.Uwp.UI.Converters">
    <converters:BoolToVisibilityConverter
        x:Key="BoolToVisibilityConverter"
        FalseValue="Collapsed"
        TrueValue="Visible" />
    <converters:BoolToVisibilityConverter
        x:Key="InverseBoolToVisibilityConverter"
        FalseValue="Visible"
        TrueValue="Collapsed" />
    <converters:BoolNegationConverter x:Key="BoolNegationConverter" />
    <converters1:BoolToStateConverter x:Key="BoolToStateConverter" />
    <converters1:StateToOpacityConverter x:Key="StateToOpacityConverter" />
    <converters1:StateToBrushConverter x:Key="StateToBrushConverter" />
    <converters1:BoolToCommandBarDisplayModeConverter x:Key="BoolToCommandBarDisplayModeConverter" />
</ResourceDictionary>
